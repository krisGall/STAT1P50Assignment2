   "source": [
    "# opening file\n",
    "file = open(file='sample-file.txt', mode='r')\n",
    "\n",
    "# reading file lines\n",
    "fileLines = file.readlines()\n",
    "\n",
    "# counting lines with q\n",
    "qCount = 0\n",
    "for line in range(0,len(fileLines)):\n",
    "    if 'q' in fileLines[line]:\n",
    "        qCount += 1\n",
    "\n",
    "# outputting count of q\n",
    "print(qCount)"
   ]
